digraph G {
	overlap=true
	bgcolor="transparent"
	node [
		fontname="Arial"
		shape=box	
		height={{nodeHeightDefault}}		
		width={{nodeWidthDefault}}

		fillcolor="#e9e9e9"
		fontcolor="#333333"
		fontsize=20
		style="filled"
		color="#aaaaaa"
		imagepos="tc"
	];

	edge [
		penwidth=2	
		fontname="Arial"
		fontsize=20
		margin=0.01
		labelfloat=true
		fontcolor="#333333"
		color="#bbbbbb"
	];
