digraph G {
	overlap=true
	bgcolor="transparent"
	node [
		fontname="Arial"
		shape=box	
		width={{w 1}}
		height={{h 1}}
		fillcolor="#e9e9e9"
		fontcolor="#333333"
		fontsize=20
		style="filled"
		color="#aaaaaa"
	];

	edge [
		penwidth=2	
		fontname="Arial"
		fontsize=20
		margin=0.01
		labelfloat=true
		fontcolor="#333333"
		color="#bbbbbb"
	];
